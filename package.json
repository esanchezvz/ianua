    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prisma": "^4.13.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
